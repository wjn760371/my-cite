ndex\",\r\n  data(){\r\n    return {\r\n      items: [\r\n        {\r\n          link: \"/\",\r\n          title: \"首页\",\r\n          icon: \"home\"\r\n        },\r\n        {\r\n          link: \"/blog\",\r\n          title: \"文章\",\r\n          icon: \"blog\",\r\n          startWith: true//只要当前路径以link开头，就默认为选中状态\r\n        },\r\n        {\r\n          link: \"/about\",\r\n          title: \"关于我\",\r\n          icon: \"about\"\r\n        },\r\n        {\r\n          link: \"/project\",\r\n          title: \"项目&效果\",\r\n          icon: \"code\"\r\n        },\r\n        {\r\n          link: \"/message\",\r\n          title: \"留言板\",\r\n          icon: \"chat\"\r\n        }\r\n      ]\r\n    }\r\n  },\r\n  components:{\r\n    Icon\r\n  },\r\n  methods:{\r\n    isSelected(item){\r\n      var link = item.link.toLowerCase();//菜单的链接地址\r\n      var curPathname = location.pathname.toLowerCase();\r\n      if (item.startWith){\r\n        return curPathname.startsWith(link);\r\n      }\r\n      return curPathname === link;\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped lang=\"less\">\r\n@import \"~@/styles/mixin.less\";\r\n@import \"~@/styles/global.less\";\r\n.menu-container{\r\n  list-style: none;\r\n  margin: 24px 0;\r\n  a{\r\n    &.selected{\r\n      background: darken(@lightWords, 30%);\r\n      color: #fff;\r\n    }\r\n    padding: 0 50px;\r\n    display: block;\r\n    display: flex;\r\n    align-items: center;\r\n    height: 45px;\r\n    color: @gray;\r\n    &:hover{\r\n      background: #2d2d2d;\r\n      color: #fff;\r\n    }\r\n    .icon{\r\n      width: 24px;\r\n    }\r\n  }\r\n}\r\n</style>"]}]}