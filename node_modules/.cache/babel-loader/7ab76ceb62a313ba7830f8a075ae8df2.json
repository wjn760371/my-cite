lement(\"img\");\r\n    img.dataset.role = \"loading\";\r\n    img.src = loadingUrl;\r\n    img.className = styles.loading;\r\n    return img;\r\n}\r\n\r\n//导出指令的配置对象\r\nexport default function(el, binding){\r\n    //根据binding.value的值，决定创建或删除img元素\r\n    const curImg = getLoadingImage(el);\r\n    if (binding.value){\r\n        //如果没有img元素再创建\r\n        if(!curImg){\r\n            const img = createLoadingImg();\r\n            el.appendChild(img);\r\n        }\r\n    }else{\r\n        if (curImg){\r\n            curImg.remove();\r\n        }\r\n    }\r\n}"]}]}