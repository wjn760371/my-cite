AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,QAAA,EADA;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAEA,KAHA;AAIA,IAAA,gBAJA,4BAIA,OAJA,EAIA;AACA,UAAA,KAAA,GAAA;AACA,QAAA,IAAA,EAAA,OADA;AAEA,QAAA,KAAA,EAAA,KAAA,SAAA,CAAA;AAFA,OAAA,CADA,CAKA;;AACA,UAAA,KAAA,SAAA,CAAA,UAAA,KAAA,CAAA,CAAA,EAAA;AACA;AACA;AACA,aAAA,OAAA,CAAA,IAAA,CAAA;AACA,UAAA,IAAA,EAAA,MADA;AAEA,UAAA,KAAA,EAAA;AAFA,SAAA;AAIA,OAPA,MAOA;AACA;AACA,aAAA,OAAA,CAAA,IAAA,CAAA;AACA,UAAA,IAAA,EAAA,cADA;AAEA,UAAA,KAAA,EAAA,KAFA;AAGA,UAAA,MAAA,EAAA;AACA,YAAA,UAAA,EAAA,KAAA,SAAA,CAAA;AADA;AAHA,SAAA;AAOA;AACA;AA3BA,GAhBA;AA6CA,EAAA,YA7CA,0BA6CA;AACA,SAAA,IAAA,CAAA,IAAA,CAAA,IAAA,CAAA,UAAA,CAAA,EAAA,CAAA;AAAA,aAAA,QAAA,CAAA,CAAA,CAAA,UAAA,CAAA,GAAA,QAAA,CAAA,CAAA,CAAA,UAAA,CAAA;AAAA,KAAA;AACA,GA/CA;AAgDA,EAAA,QAAA,EAAA;AACA;AACA,IAAA,SAFA,uBAEA;AACA,UAAA,UAAA,GAAA,CAAA,KAAA,MAAA,CAAA,MAAA,CAAA,UAAA,IAAA,CAAA,CAAA,CADA,CACA;;AACA,UAAA,IAAA,GAAA,CAAA,KAAA,MAAA,CAAA,KAAA,CAAA,IAAA,IAAA,CAAA;AACA,UAAA,KAAA,GAAA,CAAA,KAAA,MAAA,CAAA,KAAA,CAAA,KAAA,IAAA,EAAA;AACA,aAAA;AACA,QAAA,UAAA,EAAA,UADA;AAEA,QAAA,IAAA,EAAA,IAFA;AAGA,QAAA,KAAA,EAAA;AAHA,OAAA;AAKA;AAXA;AAhDA,CAAA","sourcesContent":["<template>\r\n<div class=\"blog-list-container\" ref=\"mainContainer\" v-loading=\"isLoading\">\r\n  <ul>\r\n    <li v-for=\"item in data.rows\" :key=\"item.id\">\r\n      <div class=\"thumb\" v-if=\"item.thumb\">\r\n        <RouterLink :to=\"{\r\n          name:'BlogDetail',\r\n            params: {\r\n              id: item.id\r\n            }\r\n        }\">\r\n          <img v-lazy=\"item.thumb\" :alt=\"item.title\" :title=\"item.title\" />\r\n        </RouterLink>\r\n      </div>\r\n      <div class=\"main\">\r\n        <RouterLink :to=\"{\r\n          name:'BlogDetail',\r\n          params: {\r\n            id: item.id\r\n          }\r\n        }\">\r\n          <h2>{{ item.title }}</h2>\r\n        </RouterLink>\r\n        <div class=\"aside\">\r\n          <span>日期：{{ item.createDate }}</span>\r\n          <span>浏览：{{ item.scanNumber }}</span>\r\n          <span>评论：{{ item.commentNumber }}</span>\r\n          <RouterLink :to=\"{\r\n            name:'CategoryBlog',\r\n            params: {\r\n              categoryId: item.category.id\r\n            }\r\n          }\">{{ item.category.name }}</RouterLink>\r\n        </div>\r\n        <div class=\"desc\">\r\n          {{item.description}}\r\n        </div>\r\n      </div>\r\n    </li>\r\n  </ul>\r\n  <Empty v-if=\"data.rows.length === 0 && !isLoading\"/>\r\n  <Pager v-if=\"data.total\"\r\n         :total=\"data.total\"\r\n         :limit=\"routeInfo.limit\"\r\n         :current=\"routeInfo.page\"\r\n         :visible-number=\"10\"\r\n         @pageChange=\"handlePageChange\"/>\r\n</div>\r\n</template>\r\n\r\n<script>\r\nimport Pager from \"@/components/Pager\";\r\nimport fetchData from \"@/mixins/fetchData\";\r\nimport { getBlogs } from \"@/api/blog\";\r\nimport mainScroll from \"@/mixins/mainScroll\";\r\nimport Empty from \"@/components/Empty\";\r\n\r\nexport default {\r\n  name: \"BlogList\",\r\n  mixins: [fetchData({total: 0, rows: []}), mainScroll(\"mainContainer\")],\r\n  components: {\r\n    Pager,\r\n    Empty\r\n  },\r\n  watch: {\r\n    async $route(){\r\n      this.isLoading = true;\r\n      //滚动高度为0\r\n      this.$refs.mainContainer.scrollTop = 0;\r\n      this.data = await this.fetchData();\r\n      this.isLoading = false;\r\n    }\r\n  },\r\n  methods: {\r\n    async fetchData(){\r\n      return await getBlogs();\r\n    },\r\n    handlePageChange(newPage){\r\n      const query = {\r\n        page: newPage,\r\n        limit: this.routeInfo.limit,\r\n      }\r\n      //跳转到当前的分类id，当前的页容量 newPage的页码\r\n      if (this.routeInfo.categoryId === -1){\r\n        //当前没有分类\r\n        //   /article/cate/-1?page=${newPage}&limit=${this.routeInfo.limit}\r\n        this.$router.push({\r\n          name: \"Blog\",\r\n          query\r\n        })\r\n      }else{\r\n        //   /article/cate/${this.routeInfo.categoryId}/page=${newPage}&limit=${this.routeInfo.limit}\r\n        this.$router.push({\r\n          name: \"CategoryBlog\",\r\n          query,\r\n          params: {\r\n            categoryId: this.routeInfo.categoryId\r\n          }\r\n        })\r\n      }\r\n    },\r\n  },\r\n  beforeUpdate() {\r\n    this.data.rows.sort((a, b) => parseInt(b.createDate) - parseInt(a.createDate));\r\n  },\r\n  computed: {\r\n    //获取路由信息\r\n    routeInfo(){\r\n      const categoryId = +this.$route.params.categoryId || -1;//加号用来将读取到的字符串转换为数字\r\n      const page = +this.$route.query.page || 1;\r\n      const limit = +this.$route.query.limit || 20;\r\n      return {\r\n        categoryId,\r\n        page,\r\n        limit\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped lang=\"less\">\r\n@import \"~@/styles/var.less\";\r\n@import \"~@/styles/mixin.less\";\r\n.blog-list-container {\r\n  line-height: 1.7;\r\n  position: relative;\r\n  padding: 20px;\r\n  overflow-y: scroll;\r\n  width: 100%;\r\n  height: 100%;\r\n  box-sizing: border-box;\r\n  scroll-behavior: smooth;\r\n  transition: 10ms;\r\n  .scroll();\r\n  ul {\r\n    list-style: none;\r\n    margin: 0;\r\n    padding: 0;\r\n  }\r\n}\r\nli {\r\n  display: flex;\r\n  padding: 15px 0;\r\n  border-bottom: 2px solid @gray;\r\n  .thumb {\r\n    flex: 0 0 auto;\r\n    margin-right: 15px;\r\n    img {\r\n      display: block;\r\n      max-width: 200px;\r\n      border-radius: 5px;\r\n    }\r\n  }\r\n  .main {\r\n    flex: 1 1 auto;\r\n    h2 {\r\n      margin: 0;\r\n    }\r\n  }\r\n  .aside {\r\n    font-size: 12px;\r\n    color: @dark;\r\n    span {\r\n      margin-right: 15px;\r\n    }\r\n  }\r\n  .desc {\r\n    margin: 15px 0;\r\n    font-size: 14px;\r\n  }\r\n}\r\n</style>"],"sourceRoot":"src/views/Blog/components"}]}